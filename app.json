{
  "expo": {
    "name": "IPTV Smarters Pro",
    "slug": "iptv-smarters-pro",
    "version": "1.0.0",
    "orientation": "landscape",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iptv.smarters.pro"
    },
    "android": {
      "package": "com.iptv.smarters.pro",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-screen-orientation"
    ],
    "scheme": "iptvsmarters",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ff58dd62-8ead-4a6b-8d08-cc332d0eda1e"
      }
    },
    "owner": "elpop"
  }
}
